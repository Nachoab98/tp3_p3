package clase;
public class Main {
    public static void main(String[] args) {
        RelojAcelerado reloj = new RelojAcelerado(300);
        while (true) {
            System.out.println(reloj.getTiempoAcelerado());
            try {
                Thread.sleep(1000);
            } catch (InterruptedException e) {
                e.printStackTrace();
            }
        }
    }
}
